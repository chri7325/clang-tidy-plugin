<?jelly escape-by-default=true?>
<j:jelly xmlns:j="jelly:core">
    <j:if test="${from.isDisplayGraph()}">
        <div align="right">
            <p/>
            <div class="test-trend-caption">${%Clangtidy Trend}</div>
            <div>
                <a href="${from.urlName}/">
                    <img src="${from.urlName}/graph"/>
                </a>
            </div>
        </div>
    </j:if>
</j:jelly>